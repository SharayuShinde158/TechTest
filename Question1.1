public class StudentRepo { 
 private List<Student> students = new ArrayList<>(); 
 public void addStudent(Student s){ 
 students.add(s); 
 } 
 public Student searchByName(String name) { 
 for(Student stud : students)
    {  
      if(stud.getName().equals(name)){
        return stud;
      }
    }
 } 
} 
